{
  "proxy_host": null,
  "proxy_port": null,
  "jenkins_host": "http://ci.theforeman.org/",
  "jenkins_url": "http://ci.theforeman.org/",
  "hub_config": "~/.config/hub",
  "github_user": "theforeman",
  "bot_github_user": "<%= github_user -%>",
  "irc_channel": "#theforeman-dev",
  "not_mergable_cache": "~/test_pull_request_kafo_not_mergable",
  "repo_groups": {},
  "settings": {
    "test_settings": {
      "name": "test",
      "test_prefix": "Test Results:",
      "allow_multiple": true,
      "run_without_trigger": true,
      "branches": { "master": {"jenkins_job_name": "test_kafo_master_pull_request",
                                "downstream_job_name": "test_kafo_master",
                                "build_token": "<%= jenkins_build_token -%>"
                                } },
      "repo_to_orgs": {
        "kafo": ["theforeman", "katello"]
      },
      "repo_to_pull_id_param": {
        "kafo": {"git_url":"pr_git_url","ref":"pr_git_ref","number":"pr_number"}
      }
    }
  }
}
